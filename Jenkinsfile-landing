pipeline {
    agent any

    stages {
        stage('Checar URL') {
            steps {
                git 'https://github.com/AlexandreFreitasCampos/ClaroMusicaAndroid.git'
            }
        }
        stage('Validar a Landing Pages') {
            steps {
                bat 'mvn test -Dcucumber.features=src/test/resources/features/landing.feature'
            }
        }
    }
}